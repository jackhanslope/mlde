#!/usr/bin/env bash

set -euo pipefail

export WANDB_EXPERIMENT_NAME=mlde_sample

epoch=100
num_samples=1
eval_batchsize=32

output_dir=${workdir}/samples/epoch-${epoch}

python bin/predict.py \
  ${workdir} \
  --dataset dataset_hurricanes \
  --epoch ${epoch} \
  --batch-size ${eval_batchsize} \
  --num-samples ${num_samples} \
  --ensemble-member 01
